                <th>Week 17</th>

            </thead>
            <tbody>
                {{! use this for just_data}} {{!#just_data}} {{#each overall}}

                <tr>
                    <td>{{ this.Name }}</td>
                    <td>{{ this.total }}</td>
                    <td>{{ this.Week1 }}</td>
                    <td>{{ this.Week2 }}</td>
                    <td>{{ this.Week3 }}</td>
